---
layout: default
pagination:
  data: contentful
  size: 1
  alias: page
  addAllPagesToCollections: true
permalink: "{{ page.title | slug }}/"
tags: "page"
htmlClass: html
bodyClass: body

eleventyComputed:
  title: "{{ page.title}}"
---
<div class="hero">
  <h1>{{ page.title }}</h1>
  {% image "eleventy_logo.png", "The possum is Eleventyâ€™s mascot" %}
</div>

<div class="content" style="color: blue;">
  {% contentBlock page.content %}
</div>

